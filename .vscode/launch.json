{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:8003",
      "webRoot": "${workspaceRoot}",
      "sourceMaps": true
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome",
      "address": "localhost",
      "port": 8001,
      "webRoot": "${workspaceRoot}"
    }
  ]
}